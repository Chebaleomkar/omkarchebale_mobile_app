{
  "expo": {
    "name": "omkarchebale_mobile_app",
    "slug": "omkarchebale_mobile_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/web-app-manifest-512x512.png",
    "scheme": "omkarchebalemobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/web-app-manifest-512x512.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "omkarechebale.com"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sign.png",
          "imageWidth": 800,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bf030c7f-ade4-4eb8-b441-d0bd5c24fb88"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bf030c7f-ade4-4eb8-b441-d0bd5c24fb88"
    }
  }
}
